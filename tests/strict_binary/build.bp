bob_library {
    name: "bob_test_static_lib",
    srcs: ["helper.c"],
    copts: ["-DLIB_FLAG=1"],
    host_supported: true,
    build_by_default: true,
}

bob_executable {
    name: "bob_test_simple_strict_binary",
    deps: ["bob_test_static_lib"],
    srcs: ["main.c"],
    copts: ["-DBIN_FLAG=1"],
    host_supported: true,
    target_supported: false,
    build_by_default: true,
}

bob_alias {
    name: "bob_test_strict_bins",
    srcs: [
        "bob_test_simple_strict_binary",
    ],
}
