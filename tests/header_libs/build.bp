bob_alias {
    name: "bob_test_header_libs",
    srcs: [
        "hl_main",
    ],
}

bob_static_library {
    name: "hl_a",
    export_local_include_dirs: ["include"],
}

bob_static_library {
    name: "hl_b",
    export_local_include_dirs: ["include"],
    static_libs: ["hl_a"],
}

bob_binary {
    name: "hl_main",
    srcs: ["main.c"],
    static_libs: ["hl_b"],
}
