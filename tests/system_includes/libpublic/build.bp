bob_static_library {
    name: "bob_test_local_libpublic",
    export_local_system_include_dirs: [
        "public",
    ],
    local_include_dirs: [
        "include",
    ],
    srcs: ["src/bob_test_public_impl.c"],

    // Warning is issued only when building this target.
    cflags: ["-Wconversion"],

    builder_android_bp: {
        /* AOSP builds with Werror */
        cflags: ["-Wno-error"],
    },
}
